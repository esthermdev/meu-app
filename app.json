{
  "expo": {
    "name": "Maine Ultimate",
    "slug": "meu-app",
    "version": "2.0.5",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "meu.app",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icons/splash-icon-dark.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.esmdev.TourneyAppMEU",
      "icon": "./assets/icons/ios-light.png",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.esmdev.TourneyAppMEU",
      "googleServicesFile": "./google-services.json",
      "versionCode": 41,
      "compileSdkVersion": 35,
      "targetSdkVersion": 35
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-splash-screen",
      "expo-secure-store",
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "16c193f8-cab3-49c3-afed-39fcf60c01b5"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/16c193f8-cab3-49c3-afed-39fcf60c01b5",
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "ON_LOAD",
      "enabled": true
    },
    "owner": "esmdev"
  }
}
